<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="PhysicalRegattaPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>afpa.ecf.physicalregatta.model.Auditor</class>
    <class>afpa.ecf.physicalregatta.model.Owner</class>
    <class>afpa.ecf.physicalregatta.model.Jury</class>
    <class>afpa.ecf.physicalregatta.model.Report</class>
    <class>afpa.ecf.physicalregatta.model.Serie</class>
    <class>afpa.ecf.physicalregatta.model.Class</class>
    <class>afpa.ecf.physicalregatta.model.Sailboat</class>
    <class>afpa.ecf.physicalregatta.model.Person</class>
    <class>afpa.ecf.physicalregatta.model.Compete</class>
    <class>afpa.ecf.physicalregatta.model.Club</class>
    <class>afpa.ecf.physicalregatta.model.Challenge</class>
    <class>afpa.ecf.physicalregatta.model.Secretary</class>
    <class>afpa.ecf.physicalregatta.model.Regatta</class>
    <class>afpa.ecf.physicalregatta.model.Committee</class>
    <class>afpa.ecf.physicalregatta.model.Entrant</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/physicalregatta?zeroDateTimeBehavior=convertToNull"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value="admin"/>
    </properties>
  </persistence-unit>
</persistence>
